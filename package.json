{
  "name": "3dti-toolkit",
  "version": "0.1.0",
  "description": "JavaScript port of the 3D Tune-In Toolkit",
  "main": "build/3dti-toolkit.min.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "lint": "eslint src examples",
    "clean": "rimraf 'build/*' '!build/.gitkeep'",
    "prebuild": "npm run clean",
    "build": "npm-run-all --parallel 'build:toolkit:*' 'build:extensions'",
    "build:toolkit:binaural": "sh ./compile-binaural.sh",
    "build:extensions": "babel --out-dir build src",
    "example:proxy:build": "browserify -t babelify -p [sourcemapify --root 'examples/3d-panner-proxy/' --base 'examples/3d-panner-proxy/'] --debug -e ./examples/3d-panner-proxy/3d-panner-proxy.js -o ./examples/3d-panner-proxy/3d-panner-proxy.bundle.js",
    "example:proxy:build:w": "watchify -t babelify -p [sourcemapify --root 'examples/3d-panner-proxy/' --base 'examples/3d-panner-proxy/'] --debug -e ./examples/3d-panner-proxy/3d-panner-proxy.js -o ./examples/3d-panner-proxy/3d-panner-proxy.bundle.js",
    "example:proxy:serve": "http-server -p 8341 .",
    "example:proxy:open": "opn http://127.0.0.1:8341/examples/3d-panner-proxy/",
    "example:proxy:dev": "npm-run-all --parallel 'example:proxy:build:w' 'example:proxy:serve' 'example:proxy:open'",
    "example:hls:build": "browserify -t babelify -p [sourcemapify --root 'examples/hls/' --base 'examples/hls/'] --debug -e ./examples/hls/hls.js -o ./examples/hls/hls.bundle.js",
    "example:hls:build:w": "watchify -t babelify -p [sourcemapify --root 'examples/hls/' --base 'examples/hls/'] --debug -e ./examples/hls/hls.js -o ./examples/hls/hls.bundle.js",
    "example:hls:serve": "http-server -p 8342 .",
    "example:hls:open": "opn http://127.0.0.1:8342/examples/hls/",
    "example:hls:dev": "npm-run-all --parallel 'example:hls:build:w' 'example:hls:serve' 'example:hls:open'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3DTune-In/3DTI_JavaScript_Wrapper.git"
  },
  "author": "Alexander Wallin <office@alexanderwallin.com> (http://alexanderwallin.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/3DTune-In/3DTI_JavaScript_Wrapper/issues"
  },
  "homepage": "https://github.com/3DTune-In/3DTI_JavaScript_Wrapper#readme",
  "dependencies": {
    "audio-param-shim": "^1.0.4",
    "custom-audio-node-connect": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "eslint": "^3.12.0",
    "eslint-config-alexander": "^1.2.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^3.1.2",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.5.4",
    "sourcemapify": "^1.0.4",
    "watchify": "^3.7.0"
  }
}
